<div id="page_wrapper">

  <% @posts.each do |post| %>
  <div class="post_wrapper">
  
    <tr>
      <td><h2 class="title">
<%= link_to post.title, post %>
</h2></td>
      
     
      <p class="date"><%= post.created_at.strftime("%B,%d,%Y")%> </p>
  	   
      
    </tr>
    </div>
  <% end %>
  </div>
